<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.targeteducare">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".PracticeActivity"></activity>
        <activity android:name=".MockTestActivity" />
        <activity android:name=".ProgressReportActivity" />
        <activity android:name=".PracticeTestActivity" />
        <activity android:name=".MainSubjectSelectionActivity" />
        <activity android:name=".SubjectSelectionActivity" />
        <activity android:name=".BoardSubtypeSelection" />
        <activity android:name=".SelectBoardActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".LoginV1Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SamplePapers" />
        <activity android:name=".QnAFeedActivity" />
        <activity android:name=".EngineeringCollegesActivity" />
        <activity android:name=".GridMainActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".ExamActivity"
            android:label="@string/title_activity_exam"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ExamListActivity" />
        <activity android:name=".AnswersheetActivity" />
        <activity android:name=".IPSettingActivity" />
    </application>

</manifest>